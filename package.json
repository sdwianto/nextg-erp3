{
    "name": "nextgen-erp",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "build:minimal": "BUILD_MODE=minimal next build",
        "build:server": "tsc -p tsconfig.server.json",
        "check": "next lint && tsc --noEmit",
        "db:generate": "prisma migrate dev",
        "db:migrate": "prisma migrate deploy",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:seed": "tsx prisma/seed.ts",
        "dev": "next dev --turbo -p 3002",
        "dev:minimal": "BUILD_MODE=minimal next dev --turbo -p 3002",
        "dev:server": "tsx src/server/index.ts",
        "dev:external": "tsx src/server/external-server.ts",
        "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\" \"npm run dev:external\"",
        "dev:clean": "node scripts/manage-ports.js start && npm run dev:all",
        "clear-db": "tsx prisma/clear-database.ts",
        "clear-data": "tsx prisma/clear-data.ts",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "postinstall": "prisma generate",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "lint:fix-auto": "node scripts/fix-typescript-errors.js",
        "lint:quick-fix": "node scripts/quick-fix-errors.js",
        "lint:comprehensive": "node scripts/comprehensive-fix.js",
        "lint:fix-parsing": "node scripts/fix-parsing-errors.js",
        "lint:fix-remaining": "node scripts/fix-remaining-errors.js",
        "lint:fix-websocket": "node scripts/fix-websocket-hygiene.mjs",
        "pre-deploy": "node scripts/pre-deploy-check.mjs",
        "lint:dev": "NODE_ENV=development next lint",
        "lint:prod": "NODE_ENV=production next lint",
        "check:errors": "node scripts/quick-error-check.mjs",
        "check:build-errors": "node scripts/check-build-errors.js",
        "preview": "next build && next start",
        "start": "next start",
        "start:server": "node dist/server/index.js",
        "start:external": "node dist/server/external-server.js",
        "typecheck": "tsc --noEmit",
        "ports:check": "node scripts/manage-ports.js check",
        "ports:free": "node scripts/manage-ports.js free",
        "ports:status": "node scripts/manage-ports.js status"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.7.2",
        "@hookform/resolvers": "^5.0.1",
        "@prisma/client": "^6.5.0",
        "@radix-ui/react-alert-dialog": "^1.1.13",
        "@radix-ui/react-dialog": "^1.1.13",
        "@radix-ui/react-dropdown-menu": "^2.1.14",
        "@radix-ui/react-label": "^2.1.6",
        "@radix-ui/react-progress": "^1.1.2",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.6",
        "@radix-ui/react-slot": "^1.2.2",
        "@radix-ui/react-switch": "^1.2.6",
        "@radix-ui/react-tabs": "^1.1.12",
        "@radix-ui/react-tooltip": "^1.2.6",
        "@t3-oss/env-nextjs": "^0.12.0",
        "@tanstack/react-query": "^5.69.0",
        "@tanstack/react-table": "^8.13.2",
        "@trpc/client": "^11.4.4",
        "@trpc/next": "^11.4.4",
        "@trpc/react-query": "^11.4.4",
        "@trpc/server": "^11.4.4",
        "@types/react-window": "^1.8.8",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cors": "^2.8.5",
        "critters": "^0.0.23",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.1",
        "exceljs": "^4.4.0",
        "express": "^4.21.2",
        "express-rate-limit": "^7.4.1",
        "file-saver": "^2.0.5",
        "framer-motion": "^11.0.8",
        "helmet": "^8.1.0",
        "html2canvas": "^1.4.1",
        "jsonwebtoken": "^9.0.2",
        "jspdf": "^3.0.1",
        "lucide-react": "^0.511.0",
        "morgan": "^1.10.1",
        "next": "^15.2.3",
        "next-auth": "5.0.0-beta.25",
        "next-themes": "^0.4.6",
        "nuqs": "^2.4.3",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.56.4",
        "react-hot-toast": "^2.4.1",
        "react-image-crop": "^11.0.5",
        "react-qrcode-logo": "^3.0.0",
        "react-virtualized-auto-sizer": "^1.0.20",
        "react-window": "^1.8.11",
        "recharts": "^2.12.7",
        "socket.io": "4.7.5",
        "socket.io-client": "4.7.5",
        "sonner": "^2.0.4",
        "superjson": "^2.2.1",
        "tailwind-merge": "^3.3.0",
        "zod": "^3.24.4",
        "zustand": "^5.0.4"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@next/eslint-plugin-next": "^15.5.0",
        "@tailwindcss/postcss": "^4.0.15",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.23",
        "@types/file-saver": "^2.0.7",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^20.14.10",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "concurrently": "^9.1.0",
        "eslint": "^9.23.0",
        "eslint-config-next": "^15.2.3",
        "eslint-plugin-react-refresh": "^0.4.20",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "prisma": "^6.5.0",
        "tailwindcss": "^4.0.15",
        "tsx": "^4.7.1",
        "tw-animate-css": "^1.3.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0"
    },
    "ct3aMetadata": {
        "initVersion": "7.39.3"
    },
    "packageManager": "npm@10.9.0"
}
